{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon --trace-warnings index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed:dev": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a watermyplantbw",
    "rollbackh": "heroku run knex migrate:rollback -a watermyplantbw",
    "databaseh": "heroku pg:psql -a watermyplantbw",
    "seedh": "heroku run knex seed:run -a watermyplantbw",
    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "14.3.0",
    "express": "4.17.2",
    "helmet": "5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.10",
    "knex-cleaner": "1.3.1",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "jest": "27.4.7",
    "nodemon": "2.0.15",
    "supertest": "6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/build-week-scaffolding-node.git"
  }
}
